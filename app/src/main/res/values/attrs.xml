<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ActionBar">
        <attr name="left_image" format="reference|integer"></attr>
        <attr name="text" format="reference|string"></attr>
        <attr name="right_image" format="reference|integer"></attr>
    </declare-styleable>

    <declare-styleable name="SisoEditText">
        <attr name="label" format="reference|string"></attr>
        <attr name="input" format="reference|string"></attr>
        <attr name="guide" format="reference|string"></attr>
        <attr name="labelColor" format="reference"></attr>
    </declare-styleable>

    <declare-styleable name="SisoCheckBox">
        <attr name="checkboxLabel" format="reference|string"></attr>
        <attr name="isChecked" format="boolean"></attr>
        <attr name="isDisabled" format="boolean"></attr>
    </declare-styleable>
    <declare-styleable name="SisoInputProgress">
        <attr name="total_stage" format="reference|integer"></attr>
        <attr name="current_stage" format="reference|integer"></attr>
    </declare-styleable>
</resources>
